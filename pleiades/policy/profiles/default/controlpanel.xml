<?xml version="1.0"?>
<object name="portal_controlpanel" meta_type="Plone Control Panel Tool">
 <configlet title="Image Handling" action_id="ImagingSettings"
    appId="ImagingSettings" category="Products" condition_expr=""
    icon_expr="string:$portal_url/image_icon.png"
    url_expr="string:${portal_url}/@@imaging-controlpanel" visible="True">
  <permission>Plone Site Setup: Imaging</permission>
 </configlet>
 <configlet title="Visual editor" action_id="kupu" appId="Kupu"
    category="Plone" condition_expr=""
    icon_expr="string:$portal_url/kupuimages/kupu_icon.gif"
    url_expr="string:${portal_url}/kupu_library_tool/kupu_config"
    visible="False" remove="True">
  <permission>Manage portal</permission>
 </configlet>
 <configlet title="LDAP Connection" action_id="ldap" appId="Plone"
    category="Products" condition_expr="" icon_expr=""
    url_expr="string:${portal_url}/@@ldap-controlpanel" visible="False"
    remove="true">
  <permission>Manage portal</permission>
 </configlet>
 <configlet title="Workflow Policies" action_id="placefulworkflow"
    appId="CMFPlacefulWorkflow" category="Products" condition_expr=""
    icon_expr="string:$portal_url/placefulworkflow_icon.png"
    url_expr="string:$portal_url/prefs_workflow_localpolicies_form"
    visible="False" remove="True">
  <permission>Manage portal</permission>
 </configlet>
</object>
